                        <VideoClicks>
                            <ClickThrough><![CDATA[http://t.cnoam.com/tracking_test?url=http%3A%2F%2Fwww.ciscomall.com.cn]]></ClickThrough>
                            <ClickTracking><![CDATA[http://tracking1/]]></ClickTracking>
                            <ClickTracking><![CDATA[http://tracking2/]]></ClickTracking>
                            <ClickTracking><![CDATA[http://tracking3/]]></ClickTracking>
